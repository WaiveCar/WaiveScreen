#!/bin/bash
sudo /usr/sbin/fai -v -s nfs://172.16.10.1/srv/fai/config softupdate | tee /tmp/installout
logpath=`tail -100 /tmp/installout | grep 'Log files are saved' | awk ' { print $NF } '`
echo
echo ================================================================
echo

sudo cat $logpath/shell.log

